---
layout: nil
---
{
	author: {
        title: "{{ page.title }}",
        email: "liuqing@liluo.me",
        github: "johnqing"
    },
    nav: [
        {
            title: "首页",
            url: "/"
        },
        {
            title: "关于我",
            url: "/about"
        }
    ],
	posts: [{% for post in site.posts%}
	    {
	        "title":"{{post.title}}",
	        "url":"{{site.url}}{{post.url}}",
	        "date":"{{post.date|date_to_string}}"
	    }{% if forloop.last == false %},{% endif %}
	{% endfor %}
	],
    links: [
        {% for link in site.links %}
            {
                "url": "{{ link.url }}",
                "title": "{{ link.title }}"
            }{% if forloop.last == false %},{% endif %}
        {% endfor %}
    ]
}